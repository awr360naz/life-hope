{
  "engines": {
    "node": "20.x"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
"scripts": {
  "front": "react-scripts start",
  "build:front": "react-scripts build",

  "dev:back": "npm run dev --prefix backend",
  "dev:front": "npm run front",
  "dev:both": "concurrently \"npm run dev:back\" \"npm run dev:front\"",

  "start:dev": "npm run dev:both",

  "start:prod": "node backend/dist/server.js",
  "start": "node backend/dist/server.js"
}


,
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "proxy": "http://127.0.0.1:4000",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
